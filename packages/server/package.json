{
  "name": "@lingo/core",
  "version": "2.2.0",
  "description": "Provide API",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross NODE_ENV=production && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{js,ts}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:process": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop lingo-server",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down"
  },
  "dependencies": {
    "@casl/ability": "^5.3.1",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^8.0.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^4.8.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bull": "^3.28.1",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "emoji-regex": "^9.2.2",
    "firebase-admin": "^9.10.0",
    "fs-extra": "^10.0.0",
    "googleapis": "^76.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "migrate-mongo": "^8.2.2",
    "mongoose": "^5.13.5",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.3",
    "@types/cache-manager": "^3.4.2",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.175",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.5.10",
    "@types/passport": "^1.0.6",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-google-oauth20": "^2.0.8",
    "@types/passport-jwt": "^3.0.5",
    "@types/request": "^2.48.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
